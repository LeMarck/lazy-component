{
  "name": "react-router-lazy-page",
  "version": "1.0.0",
  "description": "React.lazy alternative for pages with data fetching",
  "author": "Evgeny Petrov <jeysonep@gmail.com>",
  "license": "MIT",
  "main": "build/index.js",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LeMarck/lazy-component.git"
  },
  "bugs": {
    "url": "https://github.com/LeMarck/lazy-component/issues"
  },
  "homepage": "https://github.com/LeMarck/lazy-component",
  "keywords": [
    "react",
    "react-router",
    "lazy",
    "lazy-page"
  ],
  "files": [
    "build",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "rm -rf build && tsc",
    "lint": "eslint .",
    "test": "jest --coverage"
  },
  "peerDependencies": {
    "react": ">=17",
    "react-dom": ">=17",
    "react-router": "=5"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.17.0",
    "@testing-library/react": "12.1.5",
    "@types/jest": "27.5.2",
    "@types/react": "17.0.83",
    "@types/react-router": "5.1.20",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "eslint": "8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "7.37.2",
    "jest": "27.5.1",
    "prettier": "3.4.2",
    "ts-jest": "27.1.5",
    "typescript": "4.9.5"
  },
  "jest": {
    "verbose": true,
    "testEnvironment": "jsdom",
    "roots": [
      "<rootDir>"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom/extend-expect"
    ],
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx"
    ]
  }
}
